{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Rust: cargo build - aoc2020-rust"
		},
		{
			"type": "shell",
			"problemMatcher": [
				"$rustc"
			],
			"label": "Rust: cargo run - aoc2020-rust",
			"command": "cargo",
			"args": [
				"run"
				// "--release"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}